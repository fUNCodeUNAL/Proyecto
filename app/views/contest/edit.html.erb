<%= render "form", title:'Editar', button_text: 'Guardar' %>
<h1>Problemas actuales</h1>
<div>
	<%= form_tag problem_search_path, method: 'get', remote: true do %>
    <div class="input-group custom-search-form">
      <%= text_field_tag :search, "", placeholder: 'Buscar...', class: 'form-control' %>
      <%= hidden_field_tag :pageId, value: @pageId %>
      <span class="input-group-btn">
        <%= button_tag(type: "submit", class: "btn btn-default") do %>
            <i class="fa fa-search"></i>
        <% end %>
      </span>
    <% end %>
    </div>
    <div id= "search_results">
    </div>
</div>	